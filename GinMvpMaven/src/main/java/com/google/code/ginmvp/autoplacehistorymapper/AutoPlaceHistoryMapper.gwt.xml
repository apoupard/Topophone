<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='autoplacehistorymapper'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Other module inherits                                      -->
  <inherits name='com.google.gwt.place.Place' />
 
  <generate-with class="com.google.code.ginmvp.autoplacehistorymapper.server.AutoPlaceHistoryMapperGeneratedDelegateGenerator">
	  <when-type-assignable class="com.google.code.ginmvp.autoplacehistorymapper.client.AutoPlaceHistoryMapperGeneratedDelegate" />
  </generate-with>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>

</module>
