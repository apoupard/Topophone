<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.menu-entry {
			display: inline-block;
			width: 150px;
		}
		
		.menu-entry-c-gapper {
			padding: 0 4px;
		}
		
		.menu-entry-c-wrapper {
			border-top: 1px solid #999999;
			color: #999999;
			cursor: pointer;
			font: bold 12px/1.1em "Helvetica", "Arial", "sans-serif";
		}

		.menu-entry-label {
			display: inline-block;
			padding: 8px;
			line-height: 50px; 
			width: auto;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.menu-entry}">
		<div class="{style.menu-entry-c-gapper}">
			<div class="{style.menu-entry-c-wrapper}">
				<div >
					<p ui:field="entryLabel" class="{style.menu-entry-label}"></p>
				</div>
			</div>
		</div>
	</g:HTMLPanel>
	
</ui:UiBinder>