<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<title>Topophone</title>

<script type="application/dart" src="${appcontext}/web/topophone.dart"></script>
<script src="${appcontext}/packages/browser/dart.js"></script>

<link rel="stylesheet" type="text/css" href="${appcontext}/wro/all.css" />

<script type="text/javascript" src="${appcontext}/wro/all.js"></script>
<script src="${appcontext}/lib/bar-ui.js"></script>


</head>
<body>

	<div id="background"></div>
	<div id="header"></div>
	<div id="site">
		<nav></nav>
		<div id="section"></div>
	</div>
	<div id="footer">
	</div>
  <script type='text/javascript'>
    function startCarousel() {
      $('.jcarousel')
        .jcarousel({
  	      wrap: 'circular'
        })
        .jcarouselAutoscroll({
          interval: 3000,
          target: '+=1',
          autostart: true
        }).on('jcarousel:lastin', 'li', function(event, carousel) {
			$(carousel.last()).next().addClass('last') ;
			$(carousel.last()).removeClass('last');
		}).on('jcarousel:firstin', 'li', function(event, carousel) {
			$(carousel.first()).next().addClass('first');
			$(carousel.first()).removeClass('first');
		});
    }
    function startSoundPlayer() {
 		window.SM2BarPlayer($('.sm2-bar-ui')[0])
 	}
 	function startShowVideo(url) {
 		if($('.youtubePopup').attr('data') != url) {
    		$('.youtubePopup').attr('data', url);
    	}
    	$('.toHide.hidden').removeClass('hidden');
    }
  </script>
</body>
</html>
