<#macro headbandCarousel logo images>
<div class="headband-carousel">
    <div class="jcarousel">
      <ul>
      	<#list images as image>
      		<li><img src="${appcontext}/${image}"></li>
      	 </#list>
      </ul>
    </div>
    <div class="absoluteCenter">
		<img class="logo" alt="" src="${appcontext}${logo}">
	</div>
  </div>
</#macro>